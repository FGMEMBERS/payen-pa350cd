<?xml version="1.0" encoding="ISO-8859-1"?>

<PropertyList>

  <path>panel.ac</path>

  <animation>
    <!-- Objets opaques -->
    <object-name>panel</object-name>

    <!-- Objets transparents -->
  </animation>

  <!-- AI -->
  <model>
    <path>Aircraft/payen-pa350cd/Models/Interior/Panel/Instruments/ai/ai.xml</path>
    <offsets>
      <x-m> -0.001 </x-m>
      <y-m>  0.000 </y-m>
      <z-m>  0.060 </z-m>
    </offsets>
  </model>
  
  <!-- alt -->
  <model>
    <path>Aircraft/payen-pa350cd/Models/Interior/Panel/Instruments/alt/altimeter.xml</path>
    <offsets>
      <x-m> 0.00 </x-m>
      <y-m> 0.10 </y-m>
      <z-m> 0.06 </z-m>
    </offsets>
  </model>

  <!-- asi -->
  <model>
    <path>Aircraft/payen-pa350cd/Models/Interior/Panel/Instruments/asi/asi.xml</path>
    <offsets>
      <x-m>  0.00 </x-m>
      <y-m> -0.10 </y-m>
      <z-m>  0.06 </z-m>
    </offsets>
  </model>

  <!-- Rpm -->
  <model>
    <path>Aircraft/payen-pa350cd/Models/Interior/Panel/Instruments/rpm/rpm.xml</path>
    <offsets>
      <x-m>  0.00 </x-m>
      <y-m>  0.05 </y-m>
      <z-m> -0.03 </z-m>
    </offsets>
  </model>
  
  <!-- VSI -->
  <model>
    <path>Aircraft/payen-pa350cd/Models/Interior/Panel/Instruments/vsi/vsi.xml</path>
    <offsets>
      <x-m>  0.00 </x-m>
      <y-m> -0.05 </y-m>
      <z-m> -0.02 </z-m>
    </offsets>
  </model>
  
  <!-- fuel -->
  <model>
    <path>Aircraft/payen-pa350cd/Models/Interior/Panel/Instruments/fuel/fuel.xml</path>
    <offsets>
      <x-m>  0.00 </x-m>
      <y-m> -0.13 </y-m>
      <z-m> -0.05 </z-m>
    </offsets>
  </model>

</PropertyList>
